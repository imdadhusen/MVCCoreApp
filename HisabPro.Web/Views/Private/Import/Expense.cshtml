@using HisabPro.Constants.Resources;
@{
    Layout = "~/Views/Private/Import/_WizardLayout.cshtml";
    ViewData["Title"] = SharedResource.LabelImportExpense;
    ViewData["SectionHeader"] = SharedResource.LabelImportExpenseWizard;
    ViewData["CardTitle"] = SharedResource.LabelExpense;
}

@section BodyInfo {
    <p class="text-muted mb-0">
        <i class="bi bi-exclamation-circle-fill text-warning me-2"></i>
        @SharedResource.LabelImportExpenseMessage <a asp-controller="Import" asp-action="Expense">@SharedResource.LabelHere</a>.
    </p>
}