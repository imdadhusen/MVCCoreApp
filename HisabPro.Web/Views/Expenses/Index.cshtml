@model HisabPro.Web.ViewModel.GridViewModel<object>
@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<p>
    <a asp-action="Save">Create New</a>
</p>

<div id="tblExpense" style="position:relative">
    @await Html.PartialAsync("_GridView", Model)
</div>

@section Scripts {
    <script>
        $('#tblExpense').gridview({
            controllerName: 'Expenses',
            titleDelete: 'Delete Expense'
        });
    </script>
}
